{
  "name": "stock-sdk",
  "version": "1.3.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "dev": "yarn build && (sleep 1 && open http://localhost:4000/playground/ &) && npx serve -l 4000 .",
    "dev:docs": "vitepress dev website",
    "build:docs": "vitepress build website",
    "build:docs:root": "DOCS_BASE=/ vitepress build website",
    "preview:docs": "vitepress preview website"
  },
  "keywords": [
    "stock",
    "sdk",
    "browser",
    "node",
    "akshare",
    "A股",
    "港股",
    "美股",
    "行情"
  ],
  "author": "chengzuopeng",
  "license": "ISC",
  "description": "Tencent qt.gtimg.cn stock quote SDK - supports both browser and Node.js",
  "homepage": "https://github.com/chengzuopeng/stock-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengzuopeng/stock-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/chengzuopeng/stock-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.15",
    "vue": "^3.5.26"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "yarn@1.22.22"
}
